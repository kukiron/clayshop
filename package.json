{
  "name": "clayshop__react-app",
  "version": "1.0.0",
  "description": "ClayShop Smart-lock web interface using REST API",
  "main": "server.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "start": "node server.js",
    "dev:client": "nodemon --exec \"webpack-dev-server --mode development\"",
    "build": "npm run clear && webpack --mode production",
    "heroku-prebuild": "npm install --only=dev",
    "heroku-postbuild": "npm run build",
    "lint": "eslint . --ext .js, .jsx",
    "lint:fix": "eslint --fix . --ext .js, .jsx"
  },
  "engines": {
    "node": "9.5.0",
    "npm": "5.6.0"
  },
  "keywords": [
    "React",
    "Redux",
    "Redux-form",
    "Material-design",
    "Authentication"
  ],
  "author": "Kafil Uddin",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.10",
    "eslint-config-latest": "^1.0.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "nodemon": "^1.14.12",
    "style-loader": "^0.20.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.2",
    "material-ui": "^1.0.0-beta.32",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "^2.2.0"
  }
}
